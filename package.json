{
  "name": "spatial-server",
  "version": "1.0.0",
  "description": "A Simple Node JS Spatial Server",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erick-otenyo/simple-node-spatial-server.git"
  },
  "keywords": [
    "Spatial",
    "Server",
    "GIS",
    "Node",
    "JS",
    "dbgeo",
    "pg-promise"
  ],
  "author": "Erick Otenyo",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "cors": "^2.8.4",
    "dbgeo": "^1.1.0",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "morgan": "^1.9.0",
    "pg-promise": "^7.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.1.1",
    "nodemon": "^1.9.2"
  },
  "bugs": {
    "url": "https://github.com/erick-otenyo/simple-node-spatial-server/issues"
  },
  "homepage": "https://github.com/erick-otenyo/simple-node-spatial-server#readme"
}
